<%= renderer.r 'docblok', lines: enum[:comments] %>
typedef <%= enum[:data_type] %> <%= enum[:identifier] %>;
enum <%= enum[:identifier] %> {
    <% enum[:body].each do |variant| %>
    <% if variant['comments'] %>
    <%= renderer.r 'docblok', lines: variant['comments'] %>
    <% end %>
    <%= variant['identifier'] %> = <%= variant['value'] %>,
    <% end %>
};
